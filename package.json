{
  "name": "@yagisumi/n7zip-native",
  "version": "0.1.0",
  "description": "TODO",
  "keywords": [],
  "engines": {
    "node": ">= 10.0.0"
  },
  "private": true,
  "main": "lib/n7zip-native",
  "module": "lib/n7zip-native.mjs",
  "types": "lib/n7zip-native.d.ts",
  "unpkg": "umd/n7zip-native.js",
  "scripts": {
    "test": "run-s prettier:check eslint:check jest",
    "jest": "jest --config config/jest.config.js",
    "mocha": "cross-env TS_NODE_FILES=true TS_NODE_PROJECT=test/tsconfig.mocha.json mocha --config config/.mocharc.json \"test/**/*.test.ts\"",
    "mocha:coverage": "nyc --nycrc-path config/nyc.config.js npm run mocha -- --no-parallel",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache .nyc_output",
    "build:cjs": "rollup -c config/rollup.config.js -f cjs -o ./lib/n7zip-native.js",
    "build:mjs": "rollup -c config/rollup.config.js -f es -o ./lib/n7zip-native.mjs",
    "build:umd": "rollup -c config/rollup.config.umd.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:check": "prettier --check \"**/*.{ts,js,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint:check": "eslint \"{src,test}/**/*.{js,ts}\"",
    "prepack": "run-s build test"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-n7zip-native",
  "homepage": "http://github.com/yagisumi/node-n7zip-native#readme",
  "license": "MIT",
  "prettier": "@yagisumi/my-config-prettier",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@yagisumi/my-config-prettier": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "mocha": "^8.2.1",
    "mocha-ui-jest": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
